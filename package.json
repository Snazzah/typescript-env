{
  "name": "typescript-env",
  "version": "1.0.0",
  "description": "Template TypeScript environment",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "author": "Snazzah",
  "license": "MIT",
  "repository": "https://github.com/Snazzah/typescript-env",
  "bugs": {
    "url": "https://github.com/Snazzah/typescript-env/issues"
  },
  "funding": {
    "url": "https://github.com/sponsors/Snazzah"
  },
  "scripts": {
    "build": "([ ! -d \"lib/\" ] || rm -r lib/*) && npx tsc",
    "build:prepare": "[ -d \"lib/\" ] || npm run build",
    "setup": "npx ts-node scripts/setup",
    "changelog": "npx ts-node scripts/changelog",
    "lint": "npx eslint --ext .ts ./src",
    "lint:fix": "npx eslint --ext .ts ./src --fix",
    "test": "npx mocha -r ts-node/register --extension ts",
    "gpr": "npx ts-node scripts/gpr",
    "prepare": "npx husky install && npm run build:prepare",
    "prepublishOnly": "([ -d \"lib/\" ] || (echo \"lib folder does not exist\" && exit 1)) && npm run lint:fix"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "chai": "^4.2.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "yarn": "^1.22.10"
  }
}
